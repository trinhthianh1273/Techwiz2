                                        <ul>
                                            @{
                                                var id = Context.Session.GetString("CustomerId");
                                                var name = Context.Session.GetString("CusFullName");
                                                var cartCount = Context.Session.GetString("CartCount");

                                                if (id == null)
                                                {
                                                    <li>
                                                        <a asp-area="" asp-controller="Customers" asp-action="Login">Login</a>
                                                    </li>
                                                    <li>
                                                        <a asp-area="" asp-controller="Customers" asp-action="Register">Register</a>
                                                    </li>
                                                    <li>
                                                        <a asp-area="" asp-controller="Employees" asp-action="Login">Manager</a>
                                                    </li>
                                                }
                                                else
                                                {
                                                    <li>
                                                        <a asp-area="" asp-controller="Customers" asp-action="Profile" asp-route-id="@id">@name</a>
                                                    </li>
                                                    //<li>
                                                    //    <a id="tg-btn-search" href="javascript:void(0)">
                                                    //        <i class="fa fa-search"></i>
                                                    //    </a>
                                                    //</li>
                                                    <li class="cart-icon">
                                                        <a asp-area="" asp-controller="Customers" asp-action="Cart">
                                                            <i class="fa fa-shopping-cart"></i>
                                                            <span class="cart-count">@cartCount</span>
                                                        </a>
                                                    </li>
                                                }
                                            }
                                        </ul>
                                    </nav>
                                </div>
                                <div class="tg-colhalf">
                                    <ul class="tg-socialicons">
                                        <li>
                                            <a href="#">
                                                <i class="fa fa-facebook"></i>
