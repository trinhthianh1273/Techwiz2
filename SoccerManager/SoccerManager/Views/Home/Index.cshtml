@{
	ViewData["Title"] = "Home Page";
	Layout = "~/Views/Shared/_Main_Layout.cshtml";
	List<Match> matches = ViewBag.Matches;
	List<Match> EarlierMatches = matches.Where(match => match.EndTime <= DateTime.Now).ToList();
	int selectedEarlierMatch = EarlierMatches[0].MatchId;

	List<Match> UpcomingMatches = matches.Where(match => match.StartTime > DateTime.Now).ToList();

	List<Player> Top10Players = ViewBag.Top10Players;

	//List<Cart> Carts = ViewBag.Carts;
}

<div id="tg-wrapper" class="tg-wrapper tg-haslayout">
	<main id="tg-main" class="tg-main tg-haslayout">
		<section class="tg-main-section tg-haslayout">
			<div class="container">
				<div class="tg-section-name">
					<h2>About soccer</h2>
				</div>
				<div class="col-sm-11 col-xs-11 pull-right">
					<div class="row">
						<div class="tg-aboutussection">
							<div class="row">
								<div class="col-md-6 col-sm-12 col-xs-12">
									<figure>
										<img src="data:image/jpeg;base64," alt="image description">
									</figure>
								</div>
								<div class="col-md-6 col-sm-12 col-xs-12">
									<div class="tg-contentbox">
										<div class="tg-section-heading"><h2>Everything about football</h2></div>
										<div class="tg-description">
											<p>Football is the world’s most popular ball game in numbers of participants and spectators. Simple in its principal rules and essential equipment, the sport can be played almost anywhere.	</p>
										</div>
										<div class="tg-btnbox">
											<a class="tg-btn" asp-area="" asp-controller="AboutSoccer" asp-action="Index"><span>read more</span></a>
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</section>
		<section class=" tg-haslayout tg-bgstyleone">
			<div class="tg-bgboxone"></div>
			<div class="tg-bgboxtwo"></div>
			<a  asp-area="" asp-controller="Products" asp-action="Shopping">
				<div class="tg-bgpattrant" style="background:url(https://cdn.tuoitre.vn/thumb_w/730/471584752817336320/2023/6/12/att00005aaa-read-only-16865363467171345937081.jpg); height: 550px;">
			</div>
			</a>
			
		</section>
		<section class="tg-main-section tg-haslayout">
			<div class="container">
				<div class="tg-section-name">
					<h2>earlier match</h2>
				</div>
				<div class="col-sm-11 col-xs-11 pull-right">
					<div class="row">
						<div class="tg-latestresult">
							<div class="row">
								<div class="col-md-8 col-sm-12 col-xs-12">
									<div class="tab-content tg-tabscontent">
										@foreach (var item in EarlierMatches)
										{
											string active = "";
											if (item.MatchId == 2)
											{
												active = "active";
											}
											<div role="tabpanel" class="tab-pane @active" id="@item.MatchId">
												<div class="tg-matchresult">
													<div class="tg-box">
														<div class="tg-score">
															<h3>@item.HomeTeamScore - @item.GuestTeamScore</h3>
															<div style="color: white; text-align: center;">
																@item.StartTime
															</div>
														</div>
														<div class="tg-teamscore">
															<strong class="tg-team-logo">
																<a asp-controller="Teams" asp-action="Details" asp-route-id="@item.HomeTeam.TeamId">
																	<img src="@Url.Content("~/images/" + item.HomeTeam.LogoUrl)" alt="@item.HomeTeam.ShortName">
																</a>
															</strong>
															<div class="tg-team-nameplusstatus">
																<h4>
																	<a asp-controller="Teams" asp-action="Details" asp-route-id="@item.HomeTeam.TeamId">
																		@{
																			if (@item.HomeTeamScore > @item.GuestTeamScore)
																			{
																				<span>@item.HomeTeam.ShortName ( Win )</span>
																			}
																			else if (@item.HomeTeamScore < @item.GuestTeamScore)
																			{
																				<span>@item.HomeTeam.ShortName ( Lose )</span>
																			}
																			else
																			{
																				<span>@item.HomeTeam.ShortName ( Tie )</span>
																			}
																		}
																	</a>
																</h4>
															</div>
															<ul class="tg-playernameplusgoadtime">
																@{
																	if (@item.HomeTeam.Player.Count > 0)
																	{
																		<li>@item.HomeTeam.Player.ElementAt(0).FullName</li>
																	}
																	else
																	{
																		<li>Player Not Found</li>
																	}
																}
															</ul>
														</div>
														<div class="tg-teamscore">
															<strong class="tg-team-logo">
																<a asp-controller="Teams" asp-action="Details" asp-route-id="@item.GuestTeam.TeamId">
																	<img src="@Url.Content("~/images/" + @item.GuestTeam.LogoUrl)" alt="@item.GuestTeam.ShortName">
																</a>
															</strong>
															<div class="tg-team-nameplusstatus">
																<h4>
																	<a asp-controller="Teams" asp-action="Details" asp-route-id="@item.GuestTeam.TeamId">
																		@{
																			if (@item.GuestTeamScore > @item.HomeTeamScore)
																			{
																				<span>@item.GuestTeam.ShortName ( Win )</span>
																			}
																			else if (@item.GuestTeamScore < @item.HomeTeamScore)
																			{
																				<span>@item.GuestTeam.ShortName ( Lose )</span>
																			}
																			else
																			{
																				<span>@item.GuestTeam.ShortName ( Tie )</span>
																			}
																		}
																	</a>
																</h4>
															</div>
															<ul class="tg-playernameplusgoadtime">
																@{
																	if (@item.GuestTeam.Player.Count > 0)
																	{
																		<li>@item.GuestTeam.Player.ElementAt(0).FullName</li>
																	}
																	else
																	{
																		<li>Player Not Found</li>
																	}
																}
															</ul>
														</div>
													</div>
												</div>
											</div>
										}
										</div>
								</div>
								<div class="col-md-4 col-sm-12 col-xs-12">
									<div id="tg-matchscrollbar" class="tg-matchscrollbar tg-allmatchstatus">
										<ul class="tg-matchtabnav" role="tablist">
											@foreach (var item in EarlierMatches)
											{
												string active = "";
												if (selectedEarlierMatch == item.MatchId)
												{
													active = "active";
												}
												selectedEarlierMatch = item.MatchId;

												<li role="presentation" class="@active">

													<a href="#@item.MatchId" aria-controls="@item.MatchName" role="tab" data-toggle="tab">
														<div class="tg-match">
															<div class="tg-box">
																<strong class="tg-teamlogo">
																	<img src="@Url.Content("~/images/" + @item.HomeTeam.LogoUrl)" alt="@item.HomeTeam.ShortName">
																</strong>
																@{
																	if (@item.HomeTeamScore > @item.GuestTeamScore)
																	{
																		<h4>win</h4>
																	}
																	else if (@item.HomeTeamScore < @item.GuestTeamScore)
																	{
																		<h4>lose</h4>
																	}
																	else
																	{
																		<h4>tie</h4>
																	}
																}
																
															</div>
															<div class="tg-box">
																<h3>@item.HomeTeamScore - @item.GuestTeamScore</h3>
																<h3>@item.StartTime</h3>
															</div>
															<div class="tg-box">
																<strong class="tg-teamlogo">
																	<img src="@Url.Content("~/images/" + @item.GuestTeam.LogoUrl)" alt="@item.GuestTeam.ShortName">
																</strong>
																@{
																	if (@item.GuestTeamScore > @item.HomeTeamScore)
																	{
																		<h4>win</h4>
																	}
																	else if (@item.GuestTeamScore < @item.HomeTeamScore)
																	{
																		<h4>lose</h4>
																	}
																	else
																	{
																		<h4>tie</h4>
																	}
																}
																
															</div>
														</div>
													</a>
												</li>
											}
											</ul>
									</div>
									<div class="tg-btnbox">
										<a class="tg-btn" class="tg-btn" asp-area="" asp-controller="Matches" asp-action="EarlierMatch">view all</span></a>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</section>
		<section class="tg-bgdark tg-haslayout " style="display: flex; flex-direction: row; justify-content: flex-start; width: 100%;">

			<div style="background:url(https://static-images.vnncdn.net/files/publish/2022/10/28/world-cup-2022-47.jpg) no-repeat;background-size: cover; height: 580px; width: 100%;">
			</div>
			@*<div style="background:url(images/shop/mu_kit.png) no-repeat;background-size: cover; height: 580px;width: 100%;">
			</div>
			<div style="background:url(images/shop/psg_kit.png) no-repeat;background-size: cover; height: 580px;width: 100%;">
			</div>
			<div style="background:url(images/shop/aston_kit.png) no-repeat;background-size: cover; height: 580px;width: 100%;">
			</div>*@
		</section>
		<section class="tg-main-section tg-haslayout">
			<div class="container">
				<div class="tg-section-name">
					<h2>upcoming match</h2>
				</div>
				<div class="col-sm-11 col-xs-11 pull-right">
					<div class="row">
						<div class="tg-latestresult">
							<div class="row">
								<div class="col-md-5 col-sm-12 col-xs-12">
									<div class="tg-contentbox">
										<div class="tg-section-heading"><h2>NOTABLE MATCHES TO WATCH</h2></div>
										<div class="tg-description" style="text-align: justify;">
											<p>Get ready for an exhilarating lineup of football showdowns that are set to ignite the pitch in the coming weeks. As the anticipation builds, fans around the world are eagerly gearing up to witness the clash of titans, tactical brilliance, and the sheer passion that only football can deliver. </p>
										</div>
										<div class="tg-btnbox">
											<a class="tg-btn" asp-area="" asp-controller="Matches" asp-action="EarlierMatch"><span>view all</span></a>
										</div>
									</div>
								</div>
								<div class="col-md-7 col-sm-12 col-xs-12">
									<div id="tg-upcomingmatch-slider" class="tg-upcomingmatch-slider tg-upcomingmatch">
										<div class="swiper-wrapper">
											@foreach (Match item in UpcomingMatches)
											{
												<div class="swiper-slide">
													<div class="tg-match">
														<div class="tg-matchdetail">
															<div class="tg-box">
																<strong class="tg-teamlogo">
																	<img src="@Url.Content("~/images/" + @item.HomeTeam.LogoUrl)" alt="@item.HomeTeam.ShortName">
																</strong>
																<h3>@item.HomeTeam.ShortName</h3>
															</div>
															<div class="tg-box">
																<h3>vs</h3>
															</div>
															<div class="tg-box">
																<strong class="tg-teamlogo">
																	<img src="@Url.Content("~/images/" + @item.GuestTeam.LogoUrl)" alt="@item.GuestTeam.ShortName">
																</strong>
																<h3>@item.GuestTeam.ShortName</h3>
															</div>
														</div>
														<div class="tg-matchhover">
															<address>@item.StartTime - @item.Stadium</address>
															<div class="tg-btnbox">
																<a class="tg-btn" asp-area="" asp-controller="Matches" asp-action="Details" asp-route-id="@item.MatchId"><span>read more</span></a>
															</div>
														</div>
													</div>
												</div>
											}
										</div>
										<div class="tg-themebtnnext"><span class="fa fa-angle-down"></span></div>
										<div class="tg-themebtnprev"><span class="fa fa-angle-up"></span></div>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</section>
		<section class=" tg-haslayout tg-bgstyletwo">
			<div class="tg-bgboxone"></div>
			<div class="tg-bgboxtwo"></div>
			<div class="tg-bgpattrant">
				<div class="container">
					<div class="row">
						<div class="tg-pointstablewrapper">
							<div class="col-sm-8 col-xs-12">
								<div class="tg-pointstable">
									<div class="tg-section-heading"><h2>top 10 scorers</h2></div>
									<div id="tg-pointstable-slider" class="tg-pointstable-slider">
										<div class="swiper-wrapper">
											<div class="swiper-slide">
												<div class="tg-pointtable">
													<div class="tg-box" style="font-weight: bold;">No.</div>
													<div class="tg-box" style="font-weight: bold;">Player</div>
													<div class="tg-box" style="font-weight: bold;">Goals</div>
													<div class="tg-box" style="font-weight: bold;">Team</div>
													<div class="tg-box" style="font-weight: bold;">Average</div>
												</div>
											</div>
											@foreach(Player item in Top10Players)
											{
												<div class="swiper-slide">
													<div class="tg-pointtable">
														<div class="tg-box">@item.Number</div>
														<div class="tg-box">@item.FullName</div>
														<div class="tg-box">@item.Scores</div>
														<div class="tg-box">@item.CurrentTeamNavigation.ShortName</div>
														<div class="tg-box">0.96</div>
													</div>
												</div>
											}
											
										</div>
										<div class="tg-themebtnnext"><span class="fa fa-angle-down"></span></div>
										<div class="tg-themebtnprev"><span class="fa fa-angle-up"></span></div>
									</div>
									<div class="tg-btnbox mb-3">
										<a class="tg-btn" asp-area="" asp-controller="Players" asp-action="Index"><span>view all</span></a>
									</div>
								</div>
							</div>
							<div class="col-sm-4 col-xs-12 hidden-xs">
								<figure>
									@{
										if(Top10Players.ElementAt(0).PlayerImage.Count() > 0)
										{
											<img src="@Url.Content("~/images/" + Top10Players.ElementAt(0).PlayerImage.ElementAt(0).ImageUrl)" alt="image description">
										}
									}
								</figure>
							</div>
						</div>
					</div>
				</div>
			</div>
		</section>
		<section class="tg-main-section tg-haslayout">
			<div class="container">
				<div class="tg-section-name">
					<h2>Latest blog / news</h2>
				</div>
				<div class="col-sm-11 col-xs-11 pull-right">
					<div class="row">
						<div class="tg-blogpost">
							<div class="row">
								<div class="col-sm-6 col-xs-12">
									<article class="tg-post">
										<figure class="tg-postimg">
											<a href="#">
												<img src="images/blog/harrykanenews.png" alt="image description">
											</a>
											<figcaption>
												<ul class="tg-postmetadata">
													<li><a href="#">by skysport</a></li>
													<li><a href="#">Premier League</a></li>
												</ul>
											</figcaption>
										</figure>
										<div class="tg-posttitle"><h3><a href="#">Harry Kane: Tottenham insist striker has permission to fly to Germany for medical ahead of potential move to Bayern Munich</a></h3></div>
										<div class="tg-description">
											<p>Harry Kane is waiting for permission from Tottenham to fly to Germany for a medical on Friday ahead of his potential move to Bayern Munich.</p>
											<p>After Kane was told that a deal had been agreed between the clubs, he decided to accept Bayern's offer.</p>
										</div>
										<a class="tg-btn" href="#">Read more</a>
									</article>
								</div>
								<div class="col-sm-6 col-xs-12">
									<article class="tg-post">
										<figure class="tg-postimg">
											<a href="#">
												<img src="images/blog/skysports-spain-netherlands_6247236.jpg" alt="image description">
											</a>
											<figcaption>
												<ul class="tg-postmetadata">
													<li><a href="#">by skysport</a></li>
													<li><a href="#">Women's WC</a></li>
												</ul>
											</figcaption>
										</figure>
										<div class="tg-posttitle"><h3><a href="#">Spain 2-1 Netherlands: Salma Paralluelo scores extra-time winner to send Jorge Vilda's side into Women's World Cup semi-finals</a></h3></div>
										<div class="tg-description">
											<p>Match report as Spain edged Netherlands 2-1 to reach the Women's World Cup semi-finals for the first time ever; Mariona Caldente gave Spain the lead on 81 minutes after VAR controversy but Stefanie van der Gragt's goal forced extra-time; </p>
										</div>
										<a class="tg-btn" href="#"><span>read more</span></a>
									</article>
								</div>
							</div>
							<div class="row">
								<div class="col-sm-6 col-xs-12">
									<article class="tg-post">
										<figure class="tg-postimg">
											<a href="#">
												<img src="images/blog/news-1.png" alt="image description">
											</a>
											<figcaption>
												<ul class="tg-postmetadata">
													<li><a href="#">by marca</a></li>
													<li><a href="#">Premier League</a></li>
												</ul>
											</figcaption>
										</figure>
										<div class="tg-posttitle"><h3><a href="#">Liverpool beat out Chelsea to sign Moises Caicedo for British record fee of $139 million</a></h3></div>
										<div class="tg-description">
											<p>Moises Caicedo was never long for Brighton and Hove Albion. During the 2022-2023 Premier League season, at age 21, Caicedo turned into a dominant force in midfield.</p>
										</div>
										<a class="tg-btn" href="#">Read more</a>
									</article>
								</div>
								<div class="col-sm-6 col-xs-12">
									<article class="tg-post">
										<figure class="tg-postimg">
											<a href="#">
												<img src="images/blog/skysports-harry-kane-tottenham_6218695.jpg" alt="image description">
											</a>
											<figcaption>
												<ul class="tg-postmetadata">
													<li><a href="#">by skysport</a></li>
													<li><a href="#">Premier League</a></li>
												</ul>
											</figcaption>
										</figure>
										<div class="tg-posttitle"><h3><a href="#">Harry Kane to Bayern Munich: Why England captain joining German champions could be perfect match</a></h3></div>
										<div class="tg-description">
											<p>With Bayern Munich having now agreed a deal with Tottenham to sign Harry Kane, we ask just how much sense does a move to Bavaria make? The transfer could solve problems for both player and club... </p>
										</div>
										<a class="tg-btn" href="#"><span>read more</span></a>
									</article>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</section>
	</main>

</div>
